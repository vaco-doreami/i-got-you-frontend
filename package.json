{
  "name": "i-got-you-client",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^13.5.0",
    "phaser": "^3.55.2",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-router-dom": "^6.3.0",
    "react-scripts": "5.0.1",
    "recoil": "^0.7.3",
    "simple-peer": "^9.11.1",
    "socket.io-client": "^4.5.1",
    "styled-components": "^5.3.5",
    "styled-reset": "^4.4.1",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "rules": {
      "no-console": "warn",
      "semi": [
        "warn",
        "always",
        {
          "omitLastInOneLineBlock": true
        }
      ],
      "semi-style": [
        "warn",
        "last"
      ],
      "quotes": [
        "warn",
        "double"
      ],
      "indent": [
        "warn",
        2,
        {
          "SwitchCase": 1
        }
      ],
      "no-multi-spaces": "warn",
      "no-multiple-empty-lines": [
        "warn",
        {
          "max": 1,
          "maxEOF": 0
        }
      ],
      "space-infix-ops": "warn",
      "comma-spacing": "warn",
      "block-spacing": "warn",
      "semi-spacing": "warn",
      "object-curly-spacing": [
        "warn",
        "always"
      ],
      "key-spacing": "warn",
      "eol-last": "warn",
      "no-undef": "warn"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
